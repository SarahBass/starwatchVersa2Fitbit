<svg>
  <!-- setting background -->
 
  
  <image id="background" href="placement.png" x="0" y="0" width="100%" height="100%" class="background" />
  

  <!-- clock information set for September Wednesday, DD MM YYYY-->
  
  <text id="myLabel" class="myLabel">
   <text id="day" x="5" y="77" class= "labelyellow"/>
   <text id="month" x="92" y="77" class= "labelyellow"/> 
   <text id="date" x="176" y="77" class= "labelyellow"/> 
   <text id="year" x="200" y="77" class= "labelyellow"/> 
   
  
  <!-- battery information -->
  <text id="batteryLabel" x="255" y="20" class="label" />
  <text id="greenbatteryLabel" x="255" y="20" class="labelgreen" />
  <text id="redbatteryLabel" x="255" y="20" class="labelred" />
  
  <!-- Data information -->
  <text id="stepsLabel" x="240" y="287" class="labelpurple" />
  <text id="firelabel" x="270" y="220" class="labellightblue" /> 
  <text id="boltlabel" x="270" y="240" class="labellightblue" />   
  <text id="heartlabel" x="270" y="260" class="labellightblue" />    
  
  <!-- Images -->

<image id="colon"  class="colon" href="minutesfile/colon.png" />
